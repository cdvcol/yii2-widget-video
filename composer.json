{
  "name": "cdvcol/yii2-widget-video",
  "description": "Yii2 widget video",
  "license": "MIT",
  "type": "yii2-extension",
  "authors": [
    {
      "name": "cdvcol",
      "email": "dev@cacko.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cdvcol\\Yii2\\Widgets\\Video\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "Cdvcol\\Yii2\\Widgets\\Video\\Bootstrap"
  },
  "require": {
    "php": "~8.1",
    "yiisoft/yii2": "~2.0.45",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^2.3.0",
    "symfony/polyfill-mbstring": "^1.25.0",
    "composer/ca-bundle": "^1.3.2",
    "ua-parser/uap-php": "^3.9",
    "scssphp/scssphp": "^1.10.0",
    "symfony/deprecation-contracts": "^3.1.0",
    "npm-asset/bootstrap": "^4.6",
    "yiisoft/yii2-bootstrap5": "@dev",
    "cacko/yii2-widget-fullscreen": "^0.3.2",
    "cacko/yii2-requirejs": "^0.1.1",
    "symfony/polyfill-php72": "^1.25.0",
    "symfony/polyfill-intl-normalizer": "^1.25.0",
    "symfony/polyfill-intl-idn": "^1.25.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
        "type": "vcs",
        "url": "https://github.com/cdvcol/yii2-requirejs"
    }
  ]
}
